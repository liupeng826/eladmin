<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.snaker.engine.entity.Surrogate" table="wf_surrogate">
        <id name="id" type="java.lang.String">
            <column name="id"/>
        </id>
        <property name="processName" type="java.lang.String" column="process_Name"/>
        <property name="operator" type="java.lang.String" column="operator"/>
        <property name="surrogate" type="java.lang.String" column="surrogate"/>
        <property name="odate" type="java.lang.String" column="odate"/>
        <property name="sdate" type="java.lang.String" column="sdate"/>
        <property name="edate" type="java.lang.String" column="edate"/>
        <property name="state" type="java.lang.Integer" column="state"/>
    </class>
</hibernate-mapping>